algoritmo "Trabalhando com Tipos de Variáveis"

var
   nome: literal
   sobrenome: literal
   nomeCompleto: literal
inicio
      Escreval("Informe seu nome: ")
      leia(nome)
      Escreval("Informe seu sobrenome: ")
      leia(sobrenome)
      nomeCompleto <- nome + " " + sobrenome
      Escreval(nomeCompleto)
fimalgoritmo
